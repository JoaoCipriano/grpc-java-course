    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.pilgrim</groupId>
        <artifactId>grpc-flix-micronaut</artifactId>
        <version>0.1</version>
    </parent>

    <artifactId>proto</artifactId>

    <build>
        <plugins>
